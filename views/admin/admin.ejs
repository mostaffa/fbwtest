		<script>
			$(document).ready(function () {
				App.init();
				// Dashboard.init();
				recalibrateTime()
				setInterval(function () {
					timer($(".time")[0])
					timer($(".time")[1])

				}, 1000)
			});
			function timer(s) {
				let seconds = parseInt(s.innerText.split(':')[3])
				let minute = parseInt(s.innerText.split(':')[2])
				let hours = parseInt(s.innerText.split(':')[1])
				let day = parseInt(s.innerText.split(':')[0])
				seconds++;
				if (seconds === 60) { seconds = 0; minute++ }
				if (minute === 60) { minute = 0; hours++ }
				if (hours === 24) { hours = 0; day++ }
				if (day < 10) { day = "0" + day; }
				if (hours < 10) { hours = "0" + hours; }
				if (minute < 10) { minute = "0" + minute; }
				if (seconds < 10) { seconds = "0" + seconds; }
				// return `Seconds= ${seconds}, Minutes=${minute}`
				s.innerText = day + ":" + hours + ':' + minute + ':' + seconds;
			}
			function recalibrateTime() {
				let secondsSystem = parseInt($(".time")[0].innerText.split(':')[3])
				let minuteSystem = parseInt($(".time")[0].innerText.split(':')[2])
				let hoursSystem = parseInt($(".time")[0].innerText.split(':')[1])
				let daySystem = parseInt($(".time")[0].innerText.split(':')[0])
				let secondsServer = parseInt($(".time")[1].innerText.split(':')[3])
				let minuteServer = parseInt($(".time")[1].innerText.split(':')[2])
				let hoursServer = parseInt($(".time")[1].innerText.split(':')[1])
				let dayServer = parseInt($(".time")[1].innerText.split(':')[0])
				secondsSystem += secondsServer
				minuteSystem += minuteServer
				hoursSystem += hoursServer
				daySystem += dayServer
				if (secondsSystem >= 60) { secondsSystem = 120 - secondsSystem; minuteSystem++ }
				if (minuteSystem >= 60) { minuteSystem = 120 - minuteSystem; hoursSystem++ }
				if (hoursSystem >= 24) { hoursSystem = 48 - hoursSystem; daySystem++ }
				if (daySystem < 10) { daySystem = "0" + daySystem; }
				if (hoursSystem < 10) { hoursSystem = "0" + hoursSystem; }
				if (minuteSystem < 10) { minuteSystem = "0" + minuteSystem; }
				if (secondsSystem < 10) { secondsSystem = "0" + secondsSystem; }
				$(".time")[0].innerText = daySystem + ":" + hoursSystem + ':' + minuteSystem + ':' + secondsSystem;
			}
		</script>
